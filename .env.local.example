# Contentful CLI / Management tokens
# These values are used by the migration command in package.json.
CONTENTFUL_SPACE_ID=your_space_id
CONTENTFUL_ENVIRONMENT=master
CONTENTFUL_MANAGEMENT_TOKEN=your_content_management_token

# Contentful Delivery tokens (accessible to the browser)
# Copy the same space/env values if you only use one environment.
VITE_CONTENTFUL_SPACE_ID=your_space_id
VITE_CONTENTFUL_ENVIRONMENT=master
VITE_CONTENTFUL_DELIVERY_TOKEN=your_content_delivery_api_token

# Optional: use the preview API token if you want to query draft content.
VITE_CONTENTFUL_PREVIEW_TOKEN=your_preview_api_token
