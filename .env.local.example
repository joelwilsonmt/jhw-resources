# Contentful space configuration (used by CLI tools and server proxy)
CONTENTFUL_SPACE_ID=your_space_id
CONTENTFUL_ENVIRONMENT=your_environment_id
CONTENTFUL_MANAGEMENT_TOKEN=your_content_management_token
CONTENTFUL_DELIVERY_TOKEN=your_content_delivery_api_token
CONTENTFUL_PREVIEW_TOKEN=your_preview_api_token

# Client-side configuration (prefixed with VITE_)
# These are exposed to the browser so only use Delivery/Preview tokens.
VITE_CONTENTFUL_SPACE_ID=your_space_id
VITE_CONTENTFUL_ENVIRONMENT=your_environment_id
VITE_CONTENTFUL_DELIVERY_TOKEN=your_content_delivery_api_token
VITE_CONTENTFUL_PREVIEW_TOKEN=your_preview_api_token
